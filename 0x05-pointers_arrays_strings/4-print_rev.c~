#include "main.h"

/**
 * print_rev - prints a string, in reverse, followed by a new line.
 *
 *@s: pointer to a char.
 * Return: no return (Success)
 */
void print_rev(char *s)
{
int i;

while (s[i] != '\0')
{
i++;
}

for (i -= 1; i >= 0; i--)
{
_putchar(s[i]);
}
_putchar('\n');

/*
 *int temp, len, i;
 *len = _strlen(s);
 *
 *for (i = 0; i < len / 2; i++)
 *{
 *temp = s[i];
 *s[i] = s[len - i - 1];
 *s[len - i - 1] = temp;
 *}
*/
}
