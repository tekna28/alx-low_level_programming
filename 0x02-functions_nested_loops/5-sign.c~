#include <stdio.h>
#include "main.h"

/**
 * main - Entry point
 *
 * Return: 1 if n is greater than zero and 0 if n is zero
 * -1 if n is less than zero (Success)
 */
int print_sign(int n)
{
if (n > 0)
{
_putchar('+');
return (1);
}
else if (n == 0)
{
_putchar('0');
return (0);
}
else if (n < 0)
{
_putchar('-');
return (-1);
}
}
